		"markerjs2": "^2.32.4",
		"obj-clean": "^3.0.1",
		"rxjs": "~7.8.0",
		"uuid": "^11.1.0",
		"vanilla-cookieconsent": "^3.1.0",
		"zone.js": "0.16.0"
	},
	"devDependencies": {
		"@angular-devkit/core": "21.0.6",
